                    <Label Foreground="#0079cd" DockPanel.Dock="Bottom" FontSize="10">LEARN ABOUT COMMUNICATION SITES</Label>
                </DockPanel>
                <DockPanel Grid.Column="2" Grid.Row="2" Margin="20px" LastChildFill="False">
                    <Border DockPanel.Dock="Top" Background="#0079cd" Width="30px" HorizontalAlignment="Left">
                        <Label FontSize="9px" Foreground="White">NEW</Label>
                    </Border>
                    <Label Padding="0px 10px 0px 10px" DockPanel.Dock="Top" FontSize="17">Realizar una impresion</Label>
                    <TextBlock DockPanel.Dock="Top" FontSize="9">
                        Lorem Ipsum is simply dummy text <LineBreak/>
                        of the printing and typesetting industry.<LineBreak/>
                        Lorem Ipsum has been the industry's<LineBreak/>
                        standard dummy text ever since the 1500s,<LineBreak/>
                        when an unknown printer took
                    </TextBlock>
                    <Label Foreground="#0079cd" DockPanel.Dock="Bottom" FontSize="10">LEARN ABOUT COMMUNICATION SITES</Label>
                </DockPanel>
            </Grid>
            <Label DockPanel.Dock="Top" Margin="20px 10px 0px 0px">Image Gallery</Label>
            <DockPanel LastChildFill="False" Margin="10px">
                <Grid DockPanel.Dock="Top">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/> 
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border Margin="10px" Grid.Row="0" Grid.Column="0" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="0" Grid.Column="1" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="0" Grid.Column="2" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="0" Grid.Column="3" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="1" Grid.Column="0" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="1" Grid.Column="1" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="1" Grid.Column="2" Background="#0079cd" Width="200px" Height="100px"/>
                    <Border Margin="10px" Grid.Row="1" Grid.Column="3" Background="#0079cd" Width="200px" Height="100px"/>
                </Grid>
            </DockPanel>
        </DockPanel>
    </Grid>
</Window>
